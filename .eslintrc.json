{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "sourceType": "module",
    "ecmaVersion": 2021
  },
  "extends": ["prettier", "plugin:import", "plugin:jest/recommended"],
  "plugins": ["prettier", "import", "jest"],
  "env": {
    "node": true
  },
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn"
  }
}
