{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "sourceType": "module",
    "ecmaVersion": 2021
  },
  "extends": ["prettier", "plugin:jest/recommended"],
  "plugins": ["prettier", "jest"],
  "env": {
    "node": true
  },
  "rules": {
    "prettier/prettier": "error"
  }
}
